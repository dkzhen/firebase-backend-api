{
  "version": 2,
  "builds": [
    { "src": "app.js", "use": "@vercel/node" },
    { "src": "index.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/app/(.*)", "dest": "/app.js" },
    { "src": "/index/(.*)", "dest": "/index.js" },
    { "src": "/static/(.*)", "dest": "/static/$1" },
    { "src": "/(.*)", "dest": "/static/index.html" }
  ]
}
